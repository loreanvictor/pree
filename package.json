{
  "name": "pree",
  "version": "0.1.19",
  "description": "prebuild for no build",
  "bin": "dist/commonjs/cli/index.js",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "scripts": {
    "start": "ts-node src/cli",
    "test": "jest",
    "lint": "eslint .",
    "typecheck": "tsc -p conf/typescript/es.json --noEmit",
    "coverage": "jest --coverage",
    "build-commonjs": "tsc -p conf/typescript/commonjs.json",
    "build-es": "tsc -p conf/typescript/es.json",
    "build": "npm run build-commonjs && npm run build-es",
    "build:docs": "pree build",
    "build:docs:vercel": "vercel build --prod && vercel deploy --prebuilt --prod",
    "build:docs:surge": "pree build -c .pree.surge.yml && echo \"!.*\" > site/.surgeignore && surge site --domain https://pree.surge.sh",
    "prepack": "npm run build"
  },
  "files": [
    "dist/es",
    "dist/commonjs"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loreanvictor/pree"
  },
  "keywords": [],
  "author": "Eugene Ghanizadeh Khoub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loreanvictor/pree/issues"
  },
  "homepage": "https://loreanvictor.github.io/pree/",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/find-package-json": "^1.2.6",
    "@types/humanize-ms": "^1.2.5",
    "@types/jest": "^29.5.3",
    "@types/koa": "^2.14.0",
    "@types/libnpmsearch": "^2.0.7",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.4.2",
    "@types/ws": "^7.4.7",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-jest": "^29.6.1",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "sleep-promise": "^9.1.0",
    "ts-inference-check": "^0.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "filesize": "^10.1.0",
    "find-package-json": "^1.2.0",
    "git-url-parse": "^14.0.0",
    "gray-matter": "^4.0.3",
    "koa": "^2.15.0",
    "libnpmsearch": "^7.0.1",
    "mime": "^3.0.0",
    "minimatch": "^9.0.3",
    "ms": "^2.1.3",
    "puppeteer": "^21.9.0",
    "semver": "^7.5.4",
    "simple-git": "^3.22.0",
    "ws": "^7.5.9",
    "yaml": "^2.3.4",
    "yargs": "^17.7.2"
  }
}
