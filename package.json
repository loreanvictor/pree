{
  "name": "pree",
  "version": "0.0.9",
  "description": "prebuild for no build",
  "bin": "dist/commonjs/cli/index.js",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "scripts": {
    "start": "ts-node src/cli",
    "test": "jest",
    "lint": "eslint .",
    "typecheck": "tsc -p conf/typescript/es.json --noEmit",
    "coverage": "jest --coverage",
    "build-commonjs": "tsc -p conf/typescript/commonjs.json",
    "build-es": "tsc -p conf/typescript/es.json",
    "build": "npm run build-commonjs && npm run build-es",
    "prepack": "npm run build"
  },
  "files": [
    "dist/es",
    "dist/commonjs"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loreanvictor/pree"
  },
  "keywords": [],
  "author": "Eugene Ghanizadeh Khoub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loreanvictor/pree/issues"
  },
  "homepage": "https://github.com/loreanvictor/pree#readme",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/jest": "^29.5.3",
    "@types/koa": "^2.14.0",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-jest": "^29.6.1",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "ts-inference-check": "^0.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "filesize": "^10.1.0",
    "gray-matter": "^4.0.3",
    "koa": "^2.15.0",
    "mime": "^3.0.0",
    "puppeteer": "^21.9.0"
  }
}
